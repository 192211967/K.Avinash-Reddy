#include <stdio.h>
void largest_element_in_array()
{
    int i,arr[] = {1,2,3,4,5};
    int n = sizeof(arr) / sizeof(arr[0]);
    int largest = arr[0];
    for (i = 1; i < n; i++) 
	{
        if (arr[i] > largest) 
		{
            largest = arr[i];
        }
    }
    printf("The largest element in the array is: %d", largest);
    return 0;
}
void main()
{
    largest_element_in_array();
}
