#include<stdio.h>
int main()
{
	int vote_age;
	printf("enter the age of the candidate :");
	scanf("%d",&vote_age);
	if(vote_age<18)
	{
		printf("you are not eligible to caste your vote\n");
		printf("you are eligible to vote after %d year\n",18-vote_age);
	}
	else
	{
		printf("congratulations you are eligible for voting");
	}
	return 0;
}
